&GLOBAL
  PROJECT BENZENE-UNPERTURBED
  RUN_TYPE ENERGY_FORCE
  PRINT_LEVEL low
&END GLOBAL

&FORCE_EVAL
  METHOD QS
  &SUBSYS
    &KIND H
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND H1
      ELEMENT H
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND C
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND N
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PBE-q5
    &END KIND
    &CELL
      ABC 30.0 30.0 30.0
      PERIODIC NONE
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME ben.xyz
      COORD_FILE_FORMAT xyz
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
    &END TOPOLOGY
  &END SUBSYS

  #&PROPERTIES
  #  &TDDFPT
  #     NSTATES      5
  #     MAX_ITER    100
  #     CONVERGENCE [eV] 1.0e-6
  #     RKS_TRIPLETS .FALSE.
  #     &PRINT
  #	&ITERATION_INFO HIGH
  #    	   FILENAME ./iter.dat
  #   	&END ITERATION_INFO
  #     &END PRINT
  #  &END TDDFPT
  #&END PROPERTIES


  &DFT
    BASIS_SET_FILE_NAME /shared/ucl/apps/cp2k/7.1/gnu-4.9.2/cp2k-7.1/data/BASIS_SET
    POTENTIAL_FILE_NAME /shared/ucl/apps/cp2k/7.1/gnu-4.9.2/cp2k-7.1/data/POTENTIAL
    CHARGE 0
    LSD
    MULTIPLICITY 2
    &QS
      EPS_DEFAULT 1.0E-10
    &END QS
    &POISSON
      PERIODIC NONE
      POISSON_SOLVER wavelet
      &WAVELET
        SCF_TYPE 100
      &END WAVELET
    &END POISSON
    &MGRID
      CUTOFF 800
      NGRIDS 6
    &END MGRID
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-10
      MAX_SCF 100
      ADDED_MOS 20
      &DIAGONALIZATION T
        ALGORITHM STANDARD
      &END DIAGONALIZATION
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC

    &PRINT
      &AO_MATRICES
    #     #ADD_LAST NUMERIC
    #     #DENSITY .TRUE.
         KOHN_SHAM_MATRIX .TRUE.
    #	 OVERLAP .TRUE.
    #     #KINETIC_ENERGY .TRUE.
    #     #MATRIX_VXC .TRUE.
    #     #POTENTIAL_ENERGY .TRUE.
    #     #CORE_HAMILTONIAN
    #     #NDIGITS 30
    	 FILENAME KS_MAT
      &END AO_MATRICES
    #  &MO
    #   EIGENVECTORS .TRUE.
    #   FILENAME ./EIGEN.dat
    #  &END MO
    &END PRINT

  &END DFT
&END FORCE_EVAL